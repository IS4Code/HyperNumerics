Func<string, string> emptyConstraints = fn => "";
var data = new[] {
	new{name = "HyperComplex", args = "", hasinner = true, constraints = emptyConstraints},
	new{name = "HyperDiagonal", args = "", hasinner = true, constraints = emptyConstraints},
